{
  "name": "eslint-config",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "ts-node-dev --respawn src",
    "build:watch": "tsc --project ./tsconfig.json --watch",
    "build": "tsc --project ./tsconfig.json && eslint src/**/*.*",
    "lint": "eslint src/**/*.*"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^4.20.0",
    "eslint": "^7.23.0",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.3"
  }
}
